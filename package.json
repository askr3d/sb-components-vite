    "eslint-plugin-react-refresh": "^0.4.19",
    "eslint-plugin-storybook": "^0.11.6",
    "globals": "^15.15.0",
    "playwright": "^1.51.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "rimraf": "^6.0.1",
    "storybook": "^8.6.7",
    "typescript": "~5.7.2",
