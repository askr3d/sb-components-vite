  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "keywords": [
    "react",
    "dev",
    "askr3d"
  ],
  "license": "MIT",
  "repository": {
    "url": "https://github.com/askr3d",
    "type": "git"
  },
  "files": [
    "dist/"
  ],
  "release": {
    "branches": [
      "main"
    ]
  },
  "peerDependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "scripts": {
    "dev": "npm run storybook",
    "build": "npm run clean && tsc -b && npm run copyfiles",
